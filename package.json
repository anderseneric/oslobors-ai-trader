{
  "name": "swing-trading",
  "version": "1.0.0",
  "description": "Oslo BÃ¸rs swing trading application with AI analysis",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"node backend/server.js\" \"cd backend/python && python app.py\" --names \"backend,python\" --prefix-colors \"cyan,yellow\"",
    "backend": "node backend/server.js",
    "start": "concurrently \"node backend/server.js\" \"cd backend/python && python app.py\" --names \"backend,python\" --prefix-colors \"cyan,yellow\"",
    "frontend": "cd frontend && npm run dev",
    "python": "cd backend/python && python app.py",
    "agents": "node agents/index.js",
    "setup": "npm install && cd frontend && npm install && cd ../backend/python && pip install -r requirements.txt",
    "dev:full": "concurrently \"node backend/server.js\" \"cd backend/python && python app.py\" \"cd frontend && npm run dev\" --names \"backend,python,frontend\" --prefix-colors \"cyan,yellow,magenta\""
  },
  "keywords": [
    "swing-trading",
    "oslo-bors",
    "stocks",
    "ai-analysis"
  ],
