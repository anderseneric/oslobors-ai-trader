  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^24.34.0",
    "sqlite3": "^5.1.7",
    "yahoo-finance2": "^2.4.3"
  },
  "devDependencies": {
